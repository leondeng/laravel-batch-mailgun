{
    "name": "leondeng/laravel-batch-mailgun",
    "description": "Enable batch sending of Mailgun transport for Laravel",
    "keywords": ["laravel", "mail", "mailgun", "batch"],
    "type": "project",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "LeonDeng\\LaravelBatchMailgun\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fan Deng",
            "email": "leondeng@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "LeonDeng\\LaravelBatchMailgun\\MailServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^5.5",
        "guzzlehttp/guzzle": "^6.3"
    }
}
